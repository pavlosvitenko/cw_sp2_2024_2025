STARTPROGRAM pppp1; 
STARTBLOCK VARIABLE INT_4 aaaa0, bbbb1, cccc3, maxx4;
  READ ( aaaa0 );
  READ ( bbbb1 );
  READ ( cccc3 );

  IF aaaa0 GT bbbb1 & aaaa0 GT cccc3 {
     aaaa0 =: maxx4;
  } ELSE IF bbbb1 GT aaaa0 & bbbb1 GT cccc3 {
     bbbb1 =: maxx4;
  } ELSE {
     cccc3 =: maxx4;
  }

  WRITE ( maxx4 );

  IF ( aaaa0 EQ bbbb1 ) & ( aaaa0 EQ cccc3 ) & ( bbbb1 EQ cccc3 ) {
    WRITE ( 1 );
  } ELSE {
    WRITE ( 0 );
  }
    
  IF ( aaaa0 LT 0 ) | ( bbbb1 LT 0 ) | ( cccc3 LT 0 ) {
    WRITE ( -1 );
  } ELSE {
    WRITE ( 0 );
  }

  IF !( aaaa0 LT ( bbbb1 ADD cccc3 )) {
    WRITE ( 10 );
  } ELSE {
    WRITE ( 0 );
  } 
ENDBLOCK